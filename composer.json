{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.0.8",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "knplabs/doctrine-behaviors": "~1.1",
        "knplabs/knp-snappy-bundle": "^1.6",
        "liip/imagine-bundle": "^2.2",
        "owp/owp-core": "dev-master",
        "owp/owp-entry": "dev-master",
        "owp/owp-event": "dev-master",
        "owp/owp-news": "dev-master",
        "sensio/framework-extra-bundle": "^5.4",
        "symfony/console": "3.4.*",
        "symfony/dependency-injection": "4.*",
        "symfony/dotenv": "3.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.*",
        "symfony/framework-bundle": "3.4.*",
        "symfony/intl": "4.*",
        "symfony/orm-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/twig-bundle": "3.4.*",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "3.4.*",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.13"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "egeloen/ckeditor-bundle": "*",
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.*"
        }
    }
}
