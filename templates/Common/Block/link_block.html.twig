{% if links|length > 0 %}
        <div class="col-12">
            <h5>{{ 'Liens utiles' | trans }}</h5>
        </div>
        {% for link in links %}
            <div class="col-xs-12 col-sm-4 col-md-4">
                <ul class="list-unstyled quick-links">
                    <li><a href="{{ link.link }}"><i class="fa fa-angle-double-right"></i>{{ link.label }}</a></li>
                </ul>
            </div>
        {% endfor %}
{% endif %}
